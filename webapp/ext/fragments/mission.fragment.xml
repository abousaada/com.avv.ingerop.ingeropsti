<core:FragmentDefinition id="frg3" 
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
    xmlns:layout="sap.m">
    xmlns="sap.m" xmlns:core="sap.ui.core">

<layout:VBox id="idVBox" justifyContent="SpaceBetween">
    <Panel id="firstTablePanel" headerText="Etat de sous-traitance">
    <Table id="missionsTable" items="{/YourEntitySet}">
        <columns>
            <Column id="MissionColumn">
                <Text id="MissionHeader" text="Mission" />
            </Column>
            <Column id="LibelleColumn">
                <Text id="LibelleHeader" text="Libellé" />
            </Column>
            <Column id="BudgetColumn">
                <Text id="BudgetHeader" text="Budget global (recette)" />
            </Column>
            <Column id="BudgetStiColumn">
                <Text id="BudgetStiHeader" text="Budget déjà en STI" />
            </Column>
            <Column id="BudgetStColumn">
                <Text id="BudgeStHeader" text="Budget sous-traité  (général de recette)​" />
            </Column>
            <Column id="BudgetPrcColumn">
                <Text id="BudgetPrcHeader" text="Pourcentage de Budget sous-traité​" />
            </Column>
        </columns>
        <items>
            <ColumnListItem id="missiontItem">
                <cells>
                    <Text id="MissionCell" text="{Mission}" />
                    <Text id="LibelleCell" text="{Libelle}" />
                    <Text id="BudgetCell" text="{Budget}" />
                    <Text id="BudgetStiCell" text="{BudgetSti}" />
                    <Text id="BudgetStCell" text="{BudgetSt}" />
                    <Text id="BudgetPrcCell" text="{BudgetPrc}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</Panel>

<Panel id="fsecondTablePanel" headerText="Mission sous-traitée">
        <Table id="missions2Table" items="{/YourEntitySet}">
        <columns>
            <Column id="Mission2Column">
                <Text id="Mission2Header" text="Mission" />
            </Column>
            <Column id="Libelle2Column">
                <Text id="Libelle2Header" text="Libellé" />
            </Column>
            <Column id="Budget2Column">
                <Text id="Budget2Header" text="Budget global (recette)" />
            </Column>
            
        </columns>
        <items>
            <ColumnListItem id="missiont2Item">
                <cells>
                    <Text id="Mission2Cell" text="{Mission2}" />
                    <Text id="Libelle2Cell" text="{Libelle2}" />
                    <Text id="Budget2Cell" text="{Budget2}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
    </Panel>

</layout:VBox>
</core:FragmentDefinition>